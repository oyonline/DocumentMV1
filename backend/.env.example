# Database driver: mysql or postgres
DB_DRIVER=mysql
DB_DSN=docmv:docmv@tcp(127.0.0.1:3306)/docdb?parseTime=true&charset=utf8mb4&loc=Local

# To use PostgreSQL instead, uncomment below and comment out the mysql lines:
